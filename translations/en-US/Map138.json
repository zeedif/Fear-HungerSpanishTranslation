{
  "Map138.Event[3].Page[0].list.message[0].textLine[0]": "A bare-breasted new god appears before you...",
  "Map138.Event[3].Page[0].list.message[1].textLine[0]": "The woman glances over the girl briefly.",
  "Map138.Event[3].Page[0].list.message[2].textLine[0]": "\\c[7]Nilvan\\c[0]",
  "Map138.Event[3].Page[0].list.message[2].textLine[1]": "\"Oh new one...\"",
  "Map138.Event[3].Page[0].list.message[3].textLine[0]": "The girl seems nervous around the new god.",
  "Map138.Event[3].Page[0].list.message[4].textLine[0]": "\\c[7]Nilvan\\c[0]",
  "Map138.Event[3].Page[0].list.message[4].textLine[1]": "\"I must ask you a favor.\"",
  "Map138.Event[3].Page[0].list.message[5].textLine[0]": "\\c[7]Nilvan\\c[0]",
  "Map138.Event[3].Page[0].list.message[5].textLine[1]": "\"Our time has come. This form of existence isn't the answer",
  "Map138.Event[3].Page[0].list.message[5].textLine[2]": "we were looking for.\"",
  "Map138.Event[3].Page[0].list.message[6].textLine[0]": "\\c[7]Nilvan\\c[0]",
  "Map138.Event[3].Page[0].list.message[6].textLine[1]": "\"For mankind to break free, we need more.\"",
  "Map138.Event[3].Page[0].list.message[7].textLine[0]": "\\c[7]Nilvan\\c[0]",
  "Map138.Event[3].Page[0].list.message[7].textLine[1]": "\"We need someone equal in size and stature to them.\"",
  "Map138.Event[3].Page[0].list.message[8].textLine[0]": "Nilvan rubs her slightly swollen belly...",
  "Map138.Event[3].Page[0].list.message[9].textLine[0]": "\\c[7]Nilvan\\c[0]",
  "Map138.Event[3].Page[0].list.message[9].textLine[1]": "\"Please, I ask of you, just get my child out of here.\"",
  "Map138.Event[3].Page[0].list.message[10].textLine[0]": "\\c[7]Nilvan\\c[0]",
  "Map138.Event[3].Page[0].list.message[10].textLine[1]": "\"Take her to the heart of the darkness.\"",
  "Map138.Event[3].Page[0].list.message[11].textLine[0]": "\\c[7]Nilvan\\c[0]",
  "Map138.Event[3].Page[0].list.message[11].textLine[1]": "\"I would gladly gift my soul to you if you were to agree on",
  "Map138.Event[3].Page[0].list.message[11].textLine[2]": "these terms...\"",
  "Map138.Event[3].Page[0].list.message[12].textChoice[0]": "\"I agree.\"",
  "Map138.Event[3].Page[0].list.message[12].textChoice[1]": "\"I refuse.\"",
  "Map138.Event[3].Page[0].list.message[13].textLine[0]": "\\c[7]Nilvan\\c[0]",
  "Map138.Event[3].Page[0].list.message[13].textLine[1]": "\"Thank you.\"",
  "Map138.Event[3].Page[0].list.message[14].textLine[0]": "Nilvan gives the girl one last gaze.",
  "Map138.Event[3].Page[0].list.message[15].textLine[0]": "The girl backs away behind you.",
  "Map138.Event[3].Page[0].list.message[16].textLine[0]": "You get the \\c[2]Endless soul\\c[0].",
  "Map138.Event[3].Page[0].list.message[17].textLine[0]": "\\c[7]Nilvan\\c[0]",
  "Map138.Event[3].Page[0].list.message[17].textLine[1]": "\"...\"",
  "Map138.Event[3].Page[0].list.message[18].textLine[0]": "Nilvan doesn't seem pleased with this decision.",
  "Map138.Event[3].Page[0].list.message[19].textLine[0]": "You wake up from your dream."
}